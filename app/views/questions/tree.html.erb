<div class="treeview js-treeview" data-turbo-prefetch="false">
  <ul id="questions">
    <%= render QuestionNodeComponent.new(question: @question) %>
  </ul>
</div>
<id id="add-question">
  <%= link_to "Add Question", new_question_path,  data: { turbo_frame: :remote_modal }, class: "text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800" unless @question %>
</id>
